{
  "manifest_version": 2,

  "name": "Pete's Dashboard",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "contextMenus",
    "background"
  ],
  "icons": {
    "16": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  }
}